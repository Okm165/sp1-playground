{
    "editor.inlineSuggest.enabled": true,
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer",
        "editor.formatOnSave": true,
    },
    "editor.rulers": [
        100
    ],
    "rust-analyzer.check.overrideCommand": [
        "cargo",
        "clippy",
        "--workspace",
        "--message-format=json",
        "--all-features",
        "--all-targets",
        "--",
        "-A",
        "incomplete-features"
    ],
    "rust-analyzer.linkedProjects": [
        "Cargo.toml",
        "programs/cycle-tracker/Cargo.toml",
        "programs/ed-add/Cargo.toml",
        "programs/ed-decompress/Cargo.toml",
        "programs/ed25519/Cargo.toml",
        "programs/fibonacci/Cargo.toml",
        "programs/io/Cargo.toml",
        "programs/keccak-permute/Cargo.toml",
        "programs/secp256k1-add/Cargo.toml",
        "programs/secp256k1-decompress/Cargo.toml",
        "programs/secp256k1-double/Cargo.toml",
        "programs/sha-compress/Cargo.toml",
        "programs/sha-extend/Cargo.toml",
        "programs/sha2/Cargo.toml",
        "programs/ssz-withdrawals/Cargo.toml",
        "programs/zeth/Cargo.toml",
    ],
    "rust-analyzer.showUnlinkedFileNotification": false
}
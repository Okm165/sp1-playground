{
    "editor.inlineSuggest.enabled": true,
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer",
        "editor.formatOnSave": true,
    },
    "editor.rulers": [
        100
    ],
    "rust-analyzer.check.overrideCommand": [
        "cargo",
        "clippy",
        "--workspace",
        "--message-format=json",
        "--all-features",
        "--all-targets",
        "--",
        "-A",
        "incomplete-features"
    ],
    "rust-analyzer.linkedProjects": [
        "Cargo.toml",
        "examples/cycle-tracker/Cargo.toml",
        "examples/ed-add/Cargo.toml",
        "examples/ed-decompress/Cargo.toml",
        "examples/ed25519/Cargo.toml",
        "examples/fibonacci/Cargo.toml",
        "examples/io/Cargo.toml",
        "examples/keccak-permute/Cargo.toml",
        "examples/runner/Cargo.toml",
        "examples/secp-add/Cargo.toml",
        "examples/sha-compress/Cargo.toml",
        "examples/sha-extend/Cargo.toml",
        "examples/sha2/Cargo.toml",
        "examples/ssz-withdrawals/Cargo.toml"
    ]
}